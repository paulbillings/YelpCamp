<% include partials/header %>

    <h1>Campgrounds Page</h1>
    
    <button>
        <a href="/campgrounds/new">Add New Campground</a>
    </button>
    
    <% campgrounds.forEach(function(camp){ %>
        <div>
            <h4><%= camp.name %></h4>
            <img src="<%= camp.image %>">
        </div>
    <% }); %>
    
    <button>
        <a href="/">Back to HomePage</a>
    </button>
    
<% include partials/footer %>